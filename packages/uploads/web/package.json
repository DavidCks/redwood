  },
  "devDependencies": {
    "@redwoodjs/framework-tools": "workspace:*",
    "nodemon": "3.1.4",
    "react": "19.0.0-rc-f2df5694-20240916",
    "react-dom": "19.0.0-rc-f2df5694-20240916",
    "tsx": "4.19.1",
    "typescript": "5.6.2",
    "vitest": "2.0.5"
  },
