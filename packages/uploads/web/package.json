    "type": "git",
    "url": "git+https://github.com/redwoodjs/redwood.git",
    "directory": "packages/uploads/web"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn tsx ./build.mts && yarn build:types",
